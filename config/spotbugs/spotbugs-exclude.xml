<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude test classes from SpotBugs analysis -->
    <Match>
        <Class name="~.*Test$" />
    </Match>
    
    <!-- Exclude package-info files -->
    <Match>
        <Class name="~.*\.package-info$" />
    </Match>
</FindBugsFilter>
